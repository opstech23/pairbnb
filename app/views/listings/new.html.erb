<h1>Listings</h1>

<%= form_for @listing do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :country %>
	<%= f.text_field :country %>

	<%= f.label :state_or_province %>
	<%= f.text_field :state_or_province %>

	<%= f.label :max_guests %>
	<%= f.text_field :max_guests %>

	<%= f.label :price %>
	<%= f.text_field :price %>

	<% f.submit 'Submit' %>
<% end %>

